<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.madhav.com.ipl2018">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.madhav.com.ipl2018.app.IplIndia"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.madhav.com.ipl2018.view.activity.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.madhav.com.ipl2018.view.activity.MainActivity" />
        <activity android:name="com.madhav.com.ipl2018.view.activity.VideoActivity" />
        <activity android:name="com.madhav.com.ipl2018.view.activity.PointsTableActivity" />
        <activity
            android:name="com.madhav.com.ipl2018.view.activity.SquadsActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
    </application>

</manifest>